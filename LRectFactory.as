package com.laz.display{/* * Class LRectFactory * by Lisa Ziegler (lisa.ziegler@gmail.com) * Copyright 2008 */	import flash.display.Sprite;	import com.laz.display.LRectangle;	import com.laz.display.LRectVO;		public class LRectFactory extends Sprite	{				public function LRectFactory(taRectangles:Object):void		{				if(taRectangles != Array)				{					trace("running Lfactory in single object");					createRect(taRectangles);				}				else				{					//looping through all rects					for(var rects:Number = 0;rects < taRectangles.length; rects++)					{						trace("running Lfactory in array parsing");						createRect(taRectangles[rects]);					}				}		}			private function createRect(newRect:Object):void		{			//	var oRect:LRectVO = new LRectVO();				trace("Lfactory startX: " + newRect.fStartX);				var oBackFile = new LRectangle(newRect as LRectVO);				addChild(oBackFile);		}		}}